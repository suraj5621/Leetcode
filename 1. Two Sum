import java.io.*;
import java.lang.*;
import java.util.*;
class Solution {
    public int[] twoSum(int[] nums, int target) {
        int []a=new int[2];
		for(int k=0;k<nums.length;k++) {
			for(int j=k+1;j<nums.length;j++) {
				if(target==nums[k]+nums[j]) {
					a[0]=k;
                    a[1]=j;              
				}
			}
		}
        return a;
    }
    
}
